<tr>
	<td><%= line_item.product.title %></td>
	<td ><%= image_tag line_item.product.image_url_url, :class => 'product-image' %></td>
	<td>&times;<%=line_item.quantity%></td>
	<td><%= number_to_currency (line_item.total_price)%></td>
	<td><%= link_to 'Show', line_item.product %></td>
	<td><%= link_to 'Destroy', line_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
</tr>