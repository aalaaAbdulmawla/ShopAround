<ul>
  <% cache ['store', Product.latest] do %>
    <% Product.all.limit(Product.all.count - 1).each do |product|%>
      <% cache ['entry', product] do %>               
        <% if  !product.image_url.inspect.include? "big"%>
          <li>
          <%= link_to image_tag(product.image_url, :title => product.title, 
          :alt => product.title.capitalize), url_for(product) %>
            <button class="button">
              <div class="title">Add to cart</div>
              <div class="price">$9,99</div>
            </button>
           </li>
        <% end %>              
      <% end %>
    <% end%>
      <li class="last">
        <%= link_to image_tag(Product.last.image_url, :title => Product.last.title, 
        :alt => Product.last.title.capitalize), url_for(Product.last) %>
      </li>
  <% end %>
</ul>